    "start": "run-p startwp gs-w",
    "test": "cross-env TS_NODE_PROJECT=\"tsconfig-for-tasks.json\" jest",
    "tscfg": "tsc --showConfig",
    "lint": "eslint ./**/*.ts ./**/*.js"
